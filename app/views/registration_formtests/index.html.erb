<p id="notice"><%= notice %></p>

<h1>Registration Formtests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registration_formtests.each do |registration_formtest| %>
      <tr>
        <td><%= registration_formtest.name %></td>
        <td><%= registration_formtest.phone %></td>
        <td><%= link_to 'Show', registration_formtest %></td>
        <td><%= link_to 'Edit', edit_registration_formtest_path(registration_formtest) %></td>
        <td><%= link_to 'Destroy', registration_formtest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration Formtest', new_registration_formtest_path %>
