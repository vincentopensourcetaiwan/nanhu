<p id="notice"><%= notice %></p>

<h1>Registration Form Test Abcs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registration_form_test_abcs.each do |registration_form_test_abc| %>
      <tr>
        <td><%= registration_form_test_abc.name %></td>
        <td><%= registration_form_test_abc.phone %></td>
        <td><%= link_to 'Show', registration_form_test_abc %></td>
        <td><%= link_to 'Edit', edit_registration_form_test_abc_path(registration_form_test_abc) %></td>
        <td><%= link_to 'Destroy', registration_form_test_abc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration Form Test Abc', new_registration_form_test_abc_path %>
