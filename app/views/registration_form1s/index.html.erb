<p id="notice"><%= notice %></p>

<h1>Registration Form1s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registration_form1s.each do |registration_form1| %>
      <tr>
        <td><%= registration_form1.name %></td>
        <td><%= registration_form1.phone %></td>
        <td><%= link_to 'Show', registration_form1 %></td>
        <td><%= link_to 'Edit', edit_registration_form1_path(registration_form1) %></td>
        <td><%= link_to 'Destroy', registration_form1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration Form1', new_registration_form1_path %>
