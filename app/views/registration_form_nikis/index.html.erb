<p id="notice"><%= notice %></p>

<h1>Registration Form Nikis</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registration_form_nikis.each do |registration_form_niki| %>
      <tr>
        <td><%= registration_form_niki.name %></td>
        <td><%= registration_form_niki.phone %></td>
        <td><%= link_to 'Show', registration_form_niki %></td>
        <td><%= link_to 'Edit', edit_registration_form_niki_path(registration_form_niki) %></td>
        <td><%= link_to 'Destroy', registration_form_niki, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration Form Niki', new_registration_form_niki_path %>
